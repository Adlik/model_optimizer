[MASTER]
jobs=0
disable=C0103  # uppercase naming style
ignore-paths=^src/model_optimizer/models/imagenet/yolo/.*$,
             ^src/model_optimizer/utils/ptflops/.*$,
             ^src/model_optimizer/proto/.*$

[MESSAGES CONTROL]
disable = fixme,
          no-else-return,
          too-many-arguments,
          too-few-public-methods,
          too-many-locals,
          too-many-instance-attributes,
          no-member,
          unnecessary-pass,
          import-outside-toplevel,
          no-else-continue,
          similarities

[FORMAT]
max-line-length = 120

[BASIC]
good-names = i,
             j,
             k,
             o,
             x,
             x1,
             x2,
             x3,
             bn
